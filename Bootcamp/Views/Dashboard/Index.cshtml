			<div class="bottom-container">
				<div class="bottom-container__left">

                    <div class="box spending-box dashboard">
                        <div class="header-container">
                            <h3 class="section-header">Test Result</h3>
                        </div>
                        <div class="pie-chart">
                            <canvas id="myChart2" height="220px" width="220px"></canvas>
                        </div>
                        <div class="overall-spending">
                            <h4>Total Score</h4>
                            <span id="total-score"></span>
                        </div>
                        <div class="pie-chart__labels">
                            <div class="pie-chart__labels-item">
                                <div class="label">
                                    <div class="label__color first"></div>
                                    <a asp-action="GetHtmlTest" asp-controller="Test">HTML</a> 
                                </div>
                                @foreach (var test in Model.Tests)
                                {
                                    if (test.TestType == "Html")
                                    {
                                        <span id="score1"> @test.Score </span>
                                    }
                                }

                            </div>
                            <div class="pie-chart__labels-item">
                                <div class="label">
                                    <div class="label__color second"></div>
                                    <a asp-action="GetCssTest" asp-controller="Test">CSS</a>
                                </div>
                                @foreach (var test in Model.Tests)
                                {
                                    if (test.TestType == "Css")
                                    {
                                        <span id="score2"> @test.Score </span>
                                    }
                                }

                            </div>
                            <div class="pie-chart__labels-item">
                                <div class="label">
                                    <div class="label__color third"></div>
                                    <a asp-action="GetJsTest" asp-controller="Test">JavaScript</a>
                                </div>
                                @foreach (var test in Model.Tests)
                                {
                                    if (test.TestType == "Js")
                                    {
                                        <span id="score3"> @test.Score </span>
                                    }
                                }

                            </div>

						</div>
					</div>

